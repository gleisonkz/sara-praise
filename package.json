{
  "name": "sara-praise",
  "version": "0.0.0",
  "license": "MIT",
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
  "scripts": {
    "ng": "nx",
    "cypress": "npx nx run sp-web-e2e:e2e --headed --watch",
    "cypress:open": "npx nx run sp-web-e2e:e2e --watch",
    "e2e": "concurrently \"npm run docker:up && npx prisma migrate dev && npm run api \" \"wait-on http://localhost:3333/api && npx nx run sp-web-e2e:e2e\"",
    "prisma:dev:deploy": "prisma migrate deploy",
    "docker:up": "docker-compose up -d --force-recreate",
    "docker:down": "docker-compose down",
    "prisma:dev:migrate": "npx prisma migrate dev",
    "web": "nx serve sp-web",
    "api": "nx serve sp-api",
    "postinstall": "node ./decorate-angular-cli.js && ngcc --properties es2020 browser module main",
    "start": "npx nx run-many --target=serve --all=true",
    "build": "nx build",
    "test": "nx test",
    "ts-node": "ts-node --compiler-options {\\\"module\\\":\\\"commonjs\\\"}",
    "clean": "rm -rf node_modules && rm -rf .angular",
    "seed": "npx prisma migrate reset"
  },
  "private": true,
